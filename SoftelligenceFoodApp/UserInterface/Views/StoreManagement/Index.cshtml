@model IEnumerable<Store>

@{
    ViewData["Title"] = "Index";
}

    <h1>StoreManagement</h1>

    <a class="nav-link text-dark" asp-area="" asp-controller="StoreManagement" asp-action="Add">Add Store</a>
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Link)
            </th>
            <th>
                <a class="nav-link" asp-area="" asp-controller="StoreManagement" asp-action="Update">Update Store</a>
            </th>
            <th>
                <a class="nav-link" asp-area="" asp-controller="StoreManagement" asp-action="Delete">Delete</a>
            </th>
        </tr>
        
    </table>

