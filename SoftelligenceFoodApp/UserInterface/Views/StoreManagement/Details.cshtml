@model Store
@{
    ViewData["Title"] = "Details";
}

<h1>Restaurant Details:</h1>
<hr />

<table class="table">

    <tr>
        <th>Name</th>
        <th>Menu</th>
        <th>Image</th>
    </tr>
    <tr>
        <th style="font-weight:400;font-size:18px">@Model.Name</th>
        <th style="font-weight:400;font-size:18px">@Model.Menu.Hyperlink</th>
        <th><img class="img-thumbnail" src="@Model.Menu.Image" /></th>
    </tr>
</table>

